Cell[CellGroupData[{

Cell["Residuals and the Least Squares Properties", "Subsection",
 CellChangeTimes->{{3.708204239777*^9, 3.708204247064*^9}, {3.708217339407*^9,
   3.7082173554189997`*^9}},ExpressionUUID->"6d0ad313-063c-4bb7-91af-\
b9031970caff"],

Cell["\<\
The straight line that best fits the data is based of the vertical distances \
between the original data points and the regression line, these distances are \
called residuals.
To find the line of best fit for the data we\[CloseCurlyQuote]ve used thus \
far, we can try using the demonstration below and we need to find the line \
that gives us smallest value of the sum of the squares. You can change the \
slope and the y-intercept of the equation, and if you do give up, you can \
just click on the tab \[OpenCurlyDoubleQuote]show least squares line\
\[CloseCurlyDoubleQuote].
You will notice that the line with smallest sum of squares is the line of \
best fit we showed in the above graphs.\
\>", "Text",
 CellChangeTimes->{{3.708217368733*^9, 3.708217459861*^9}, {
  3.7082178362475*^9, 3.7082178388815002`*^9}, {3.7082193487775*^9, 
  3.7082193785375*^9}, {3.7082194240655003`*^9, 
  3.7082196674055*^9}},ExpressionUUID->"3942b77d-124c-44e6-be87-80a592f4d615"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"squaredResidualPlot", "[", 
   RowBox[{"data_List", ",", " ", 
    RowBox[{"int_", "?", "NumberQ"}], ",", " ", "\n", 
    RowBox[{"slope_", "?", "NumberQ"}], ",", " ", "opts___Rule"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "x", ",", "xdata", ",", "ydata", ",", "line", ",", "pred", ",", "resid", 
      ",", "i", ",", "l", ",", "p", ",", "redEdges", ",", "squares", ",", 
      "dashedEdges"}], "}"}], ",", 
    RowBox[{
     RowBox[{"xdata", "=", 
      RowBox[{"data", "[", 
       RowBox[{"[", 
        RowBox[{"All", ",", "1"}], "]"}], "]"}]}], ";", 
     RowBox[{"ydata", "=", 
      RowBox[{"data", "[", 
       RowBox[{"[", 
        RowBox[{"All", ",", "2"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"line", "=", 
      RowBox[{"int", " ", "+", " ", 
       RowBox[{"slope", "*", "x"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"slope", "==", "0"}], ",", 
       RowBox[{"pred", "=", 
        RowBox[{"Table", "[", 
         RowBox[{"int", ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", 
            RowBox[{"Length", "[", "xdata", "]"}]}], "}"}]}], "]"}]}], ",", 
       " ", 
       RowBox[{"pred", "=", 
        RowBox[{"line", " ", "/.", " ", 
         RowBox[{"x", "\[Rule]", "xdata"}]}]}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"resid", "=", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"slope", ">", "0"}], ",", " ", 
        RowBox[{"ydata", "-", "pred"}], ",", " ", 
        RowBox[{"pred", "-", "ydata"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"l", "=", 
      RowBox[{"ListPlot", "[", 
       RowBox[{"data", ",", " ", "opts", ",", 
        RowBox[{"AxesOrigin", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"0", ",", "0"}], "}"}]}], ",", 
        RowBox[{"PlotRange", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"70", ",", "95"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"70", ",", "95"}], "}"}]}], "}"}]}], ",", 
        RowBox[{"PlotStyle", "\[Rule]", 
         RowBox[{"AbsolutePointSize", "[", "4", "]"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"p", "=", 
      RowBox[{"Plot", "[", 
       RowBox[{"line", ",", " ", 
        RowBox[{"{", 
         RowBox[{"x", ",", "0", ",", "100"}], "}"}], ",", "opts", ",", 
        RowBox[{"PlotStyle", "\[Rule]", 
         RowBox[{"{", 
          RowBox[{"Hue", "[", ".7", "]"}], "}"}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"redEdges", "=", 
      RowBox[{"Prepend", "[", 
       RowBox[{
        RowBox[{"MapThread", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Line", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"#1", ",", "#3"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"#1", "-", "#4"}], ",", "#3"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"#1", "-", "#4"}], ",", "#2"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"#1", ",", "#2"}], "}"}]}], "}"}], "]"}], "&"}], ",", 
          RowBox[{"{", 
           RowBox[{"xdata", ",", "ydata", ",", "pred", ",", "resid"}], 
           "}"}]}], "]"}], ",", 
        RowBox[{"Hue", "[", "1", "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"squares", "=", 
      RowBox[{"Prepend", "[", 
       RowBox[{
        RowBox[{"MapThread", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Rectangle", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"#1", "-", "#4"}], ",", "#3"}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"#1", ",", "#2"}], "}"}]}], "]"}], "&"}], ",", 
          RowBox[{"{", 
           RowBox[{"xdata", ",", "ydata", ",", "pred", ",", "resid"}], 
           "}"}]}], "]"}], ",", 
        RowBox[{"Opacity", "[", ".2", "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"dashedEdges", "=", 
      RowBox[{"Prepend", "[", 
       RowBox[{
        RowBox[{"MapThread", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Line", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"#1", ",", "#2"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"#1", ",", "#3"}], "}"}]}], "}"}], "]"}], "&"}], ",", 
          RowBox[{"{", 
           RowBox[{"xdata", ",", "ydata", ",", "pred"}], "}"}]}], "]"}], ",", 
        RowBox[{"Dashing", "[", 
         RowBox[{"{", ".01", "}"}], "]"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Show", "[", 
      RowBox[{"l", ",", "p", ",", 
       RowBox[{"Graphics", "[", "squares", "]"}], ",", 
       RowBox[{"Graphics", "[", "redEdges", "]"}], ",", 
       RowBox[{"Graphics", "[", "dashedEdges", "]"}], ",", 
       RowBox[{"AspectRatio", "\[Rule]", "Automatic"}], ",", 
       RowBox[{"AxesLabel", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"\"\<x\>\"", ",", " ", "\"\<y\>\""}], "}"}]}], ",", 
       RowBox[{"PlotLabel", "\[Rule]", 
        RowBox[{"\"\<sum of squares: \>\"", "<>", 
         RowBox[{"ToString", "[", 
          RowBox[{"PaddedForm", "[", 
           RowBox[{
            RowBox[{"Plus", "@@", 
             RowBox[{"(", 
              SuperscriptBox["resid", "2"], ")"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"6", ",", "3"}], "}"}]}], "]"}], "]"}]}]}]}], "]"}]}]}], 
   "\n", "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"data1", "=", 
   RowBox[{"Values", "@", 
    RowBox[{"QuantityMagnitude", "@", 
     RowBox[{"Normal", "@", 
