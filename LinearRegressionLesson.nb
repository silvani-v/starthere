       "]"}]}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"data2", "=", 
   RowBox[{"Values", "@", 
    RowBox[{"QuantityMagnitude", "@", 
     RowBox[{"Normal", "@", 
      RowBox[{"btreatmentData", "[", 
       RowBox[{"All", ",", 
        RowBox[{"{", 
         RowBox[{"\"\<EntryWeight\>\"", ",", "\"\<ExitWeight\>\""}], "}"}]}], 
       "]"}]}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"data3", "=", 
   RowBox[{"Values", "@", 
    RowBox[{"QuantityMagnitude", "@", 
     RowBox[{"Normal", "@", 
      RowBox[{"ftreatmentData", "[", 
       RowBox[{"All", ",", 
        RowBox[{"{", 
         RowBox[{"\"\<EntryWeight\>\"", ",", "\"\<ExitWeight\>\""}], "}"}]}], 
       "]"}]}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"m1", ",", "b1"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"m2", ",", "b2"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"m3", ",", "b3"}], "}"}]}], "}"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{
