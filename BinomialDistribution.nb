    RowBox[{
     FractionBox[
      RowBox[{"n", "!"}], 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"n", "-", "x"}], ")"}], "!"}], 
       RowBox[{"x", "!"}]}]], 
     SuperscriptBox["p", "x"], 
     SuperscriptBox["q", 
      RowBox[{"n", "-", "x"}]], "         ", "for", " ", "x"}], "=", "0"}]}], 
